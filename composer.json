{
    "name": "textmaster/akeneo-extension",
    "type": "symfony-bundle",
    "description": "Textmaster extension for Akeneo product data",
    "license": "OSL-3.0",
    "authors": [
        {"name": "Akeneo", "homepage": "http://www.akeneo.com"}
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "akeneo/pim-community-dev": "^2.0.6",
        "worldia/textmaster-api": "^0.6.0"
    },
    "require-dev": {
        "behat/behat": "^3.1.0",
        "friendsofphp/php-cs-fixer": "^2.8.2",
        "phpspec/phpspec": "^2.5.8"
    },
    "autoload": {
        "psr-4": {
            "Pim\\Bundle\\TextmasterBundle\\": "src/"
        }
    },
    "branch-alias": {
        "dev-master": "2.0.x-dev",
        "dev-1.2": "1.2.x-dev",
        "dev-1.1": "1.1.x-dev",
        "dev-1.0": "1.0.x-dev"
    }
}
