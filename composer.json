{
    "name": "textmaster/akeneo-extension",
    "type": "symfony-bundle",
    "description": "Textmaster extension for Akeneo product data",
    "license": "OSL-3.0",
    "authors": [
        {"name": "akeneo"}
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "akeneo/pim-community-dev": "^1.5.8",
        "worldia/textmaster-api": "0.2.7"
    },
    "require-dev": {
        "behat/behat": "^3.1.0",
        "doctrine/migrations": "1.0.0-alpha3@alpha",
        "fabpot/php-cs-fixer": "^1.11",
        "phpspec/phpspec": "2.4.*"
    },
    "autoload": {
        "psr-4": {
            "Pim\\Bundle\\TextmasterBundle\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
